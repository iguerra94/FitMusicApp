<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Gray shadow. -->
    <item
        android:left="1dp"
        android:top="3dp">
        <shape>
            <solid android:color="#689F38" />
            <corners android:radius="200dp" />
        </shape>
    </item>

    <!-- White foreground.  -->
    <item
        android:bottom="3dp"
        android:right="1dp">
        <shape>
            <solid android:color="#8BC34A" />
            <corners android:radius="200dp" />
        </shape>
    </item>

</layer-list>
